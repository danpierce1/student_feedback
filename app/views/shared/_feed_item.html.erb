<li id="<%= feed_item.id %>">
  <%= link_to gravatar_for(feed_item.user), feed_item.user %>
    <span class="user">
      <%= link_to feed_item.user.name, feed_item.user %>
    </span>
    <span class="content"><strong> Strengths:</strong><%= feed_item.strengths %></span>
    <span class="content"><strong> Weaknesses:</strong><%= feed_item.weaknesses %></span>
    <span class="content"><strong> Recommendations:</strong><%= feed_item.recommendations %></span>
    <span class="content"><strong> Rating:</strong><%= feed_item.rating %></span>
        <span class="content"><strong> About Module:</strong><%= feed_item.cmodules %></span>
    <span class="timestamp">
      Posted <%= time_ago_in_words(feed_item.created_at) %> ago.
    </span>
  <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "You sure?" },
                                     title: feed_item.strengths %>
  <% end %>
</li>